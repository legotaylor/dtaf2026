{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${version}",
	"name": "${modName}",
	"description": "${modDescription}",
	"authors": [
		"${authors}"
	],
	"contributors": [
		"${contributors}"
	],
	"contact": {
		"homepage": "${homepage}",
		"issues": "${issues}",
		"sources": "${sources}"
	},
	"license": "${license}",
	"icon": "assets/${modId}/icon.png",
	"depends": {
		"minecraft": ">=${mc}",
		"fabric-api": ">=${fapi}"
	},
	"suggests": {
	},
	"entrypoints": {
		"main": [
			"dev.dannytaylor.${modId}.common.Main"
		],
		"client": [
			"dev.dannytaylor.${modId}.client.ClientMain"
		],
		"modmenu": [
			"dev.dannytaylor.${modId}.client.compatibility.ModMenu"
		]
	},
	"custom": {
		"modmenu": {
			"links": {
				"dannytaylor.donate": "https://dannytaylor.dev/donate"
			}
		}
	},
	"mixins": [
		"${modId}_client.mixins.json",
		"${modId}_common.mixins.json",
		"${modId}_iris.mixins.json"
	],
	"accessWidener" : "${modId}.accesswidener"
}
